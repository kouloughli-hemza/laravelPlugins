{
    "name": "Kouloughliapp/plugins",
    "description": "Core stuff for Kouloughli plugin support.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kouloughli Hemza",
            "email": "php7@outlook.fr",

            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^6.0",
        "symfony/process": "^4.3",
        "illuminate/console": "^6.0",
        "illuminate/filesystem": "^6.0",
        "illuminate/view": "^6.0",
        "illuminate/auth": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "kouloughli\\Plugins\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kouloughli\\Plugins\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "kouloughli\\Plugins\\PluginsServiceProvider"
            ]
        }
    }
}
